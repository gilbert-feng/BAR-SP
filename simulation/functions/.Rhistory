source_directory <- function(path, pattern = "\\.[Rr]$", recursive = FALSE) {
files <- list.files(path = path,
pattern = pattern,
full.names = TRUE,
recursive = recursive)
for (file in files) {
source(file, encoding = 'UTF-8')
cat("loading: ", file, "\n")
}
}
source_directory("functions")
